<% provide(:title, 'Index Teams') %>

<div class="container">
  <div class="row">
    <h2>Index Team</h2>
    <table>
      <tbody>
        <% @teams.each do |team| %>
          <tr>
            <td><%= team.name %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <div class="row">
    <%= link_to "New Team", new_team_path, class: "btn waves-effect waves-light" %>
  </div>
</div>
