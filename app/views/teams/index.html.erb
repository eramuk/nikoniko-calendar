<% provide(:title, 'Index Teams') %>

<div class="container">
  <div class="row">
    <h2>Index Team</h2>
    <table>
      <tbody>
        <% @teams.each do |team| %>
          <tr>
            <td><%= team.name %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>

  <div class="row">
    <%= link_to "Create Team", new_team_path %>
  </div>
</div>
